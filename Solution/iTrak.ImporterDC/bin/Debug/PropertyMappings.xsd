      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" name="Property">
          <xs:complexType>
            <xs:attribute name="SourcePropertyCode" type="xs:string" use="optional" />
            <xs:attribute name="iTrakPropertyName" type="xs:string" use="optional" />
